    <add key="mqWaitTime" value="600000"/>
    <!-- 文件服务器地址-->
    <add key="fileRemoteHost" value="\\172.21.26.125"/>
    <!-- 文件服务器用户名 -->
    <add key="fileUserName" value="APAC\BIZOrder1"/>
    <!-- 文件服务器密码 -->
    <add key="filePassword" value="WAN2020wan"/>
    <!-- 拉取的文件存放位置 -->
    <add key="fileDirName" value="CustomVersion"/>
    <!-- 替换程序名 -->
    <add key="targetAppName" value="inSight"/>
    <!-- 替换程序目录 -->
    <add key="targetAppDir" value="C:\Program Files (x86)\2020 inSight\10.4.0"/>
    <!-- 当目标程序打开时，提示信息-->
    <add key="InSightAlert" value="检测到InSight存在必需更新的版本，60秒后将自动关闭InSight！"/>
    <!-- 当目标程序打开时，待等多少秒关闭目标程序-->
    <add key="InSightWaitTime" value="60000"/>
  </appSettings>
</configuration>