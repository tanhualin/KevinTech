<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
  <appSettings>
    <add key="mqPort" value="5672"/>
    <add key="mqEndpointIP" value="172.21.26.120"/>
    <add key="mqUserName" value="admin"/>
    <add key="mqPassword" value="2020"/>
    <!--虚拟服务器-->
    <add key="mqVirtualHost" value="testhost"/>
    <!--队列名-->
    <add key="mqQueueName" value="test_queue_work1"/>
    <!--交换机名-->
    <add key="mqExchangeName" value="test_exchange_fanout"/>
    <!--暂停N秒，防止CPU爆满的问题 (毫秒)-->
    <add key="mqTimeout" value="1000"/>
